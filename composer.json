    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "guzzlehttp/guzzle": "6.*"
    },
    "keywords":
        [
            "lumen",
            "cache",
            "opcache",
            "reset",
            "clear"
        ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Giovanni Curletta",
            "email": "michele.curletta@icloud.com"
        },
        {
    	    "name": "Alex Fedorko",
    	    "email": "alex.fedorko@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MicheleCurletta\\LaravelOpcacheClear\\": "src"
        }
    }
}
